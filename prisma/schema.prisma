// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tbl_produtos {
  id        Int    @unique @default(autoincrement())
  categoria String
  tipo      String
}

model tbl_admin {
  id    Int     @unique @default(autoincrement())
  nome  String
  email String
  senha String
  foto  String?
}
